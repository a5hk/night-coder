{
  "$schema": "vscode://schemas/color-theme",
  "name": "Night Coder",
  "type": "dark",
  "colors": {
    "activityBar.activeBorder": "#080800",
    "activityBar.background": "#080800",
    "activityBar.foreground": "#d7dae0",
    "activityBarBadge.background": "#006600",
    "activityBarBadge.foreground": "#f5fff5",
    "badge.background": "#282c34",
    "button.background": "#404754",
    "debugToolBar.background": "#21252b",
    "diffEditor.insertedTextBackground": "#00809b33",
    "dropdown.background": "#111111",
    "dropdown.border": "#080800",
    "editor.background": "#080800",
    "editor.findMatchBackground": "#42557b",
    "editor.findMatchBorder": "#457dff",
    "editor.findMatchHighlightBackground": "#6199ff2f",
    "editor.foreground": "#BBBBBB",
    "editor.lineHighlightBackground": "#33333377",
    "editor.selectionBackground": "#33333399",
    "editor.selectionHighlightBackground": "#33333377",
    "editor.selectionHighlightBorder": "#dddddd",
    "editor.wordHighlightBackground": "#d2e0ff2f",
    "editor.wordHighlightBorder": "#7f848e",
    "editor.wordHighlightStrongBackground": "#BBBBBB26",
    "editor.wordHighlightStrongBorder": "#7f848e",
    "editorActiveLineNumber.foreground": "#737984",
    "editorBracketMatch.background": "#515a6b",
    "editorBracketMatch.border": "#515a6b",
    "editorCursor.background": "#ffffffc9",
    "editorCursor.foreground": "#eeeeee",
    "editorError.foreground": "#c24038",
    "editorGroup.background": "#181a1f",
    "editorGroup.border": "#080800",
    "editorGroupHeader.tabsBackground": "#080800",
    "editorHoverWidget.background": "#21252b",
    "editorHoverWidget.border": "#181a1f",
    "editorIndentGuide.activeBackground": "#53ac53",
    "editorIndentGuide.background": "#3b4048",
    "editorLineNumber.foreground": "#495162",
    "editorMarkerNavigation.background": "#21252b",
    "editorOverviewRuler.border": "#080800",
    "editorRuler.foreground": "#BBBBBB26",
    "editorSuggestWidget.background": "#000000",
    "editorSuggestWidget.border": "#181a1f",
    "editorSuggestWidget.foreground": "#cccccc",
    "editorSuggestWidget.highlightForeground": "#eeeeee",
    "editorSuggestWidget.selectedBackground": "#555555",
    "editorWarning.foreground": "#CABD2B",
    "editorWhitespace.foreground": "#666666",
    "editorWidget.background": "#000000",
    "editorWidget.border": "#080800",
    "editorWidget.resizeBorder": "#006600",
    "focusBorder": "#080800",
    "input.background": "#111111",
    "list.activeSelectionBackground": "#333333",
    "list.activeSelectionForeground": "#d7dae0",
    "list.focusBackground": "#383e4a",
    "list.highlightForeground": "#c5c5c5",
    "list.hoverBackground": "#444444",
    "list.inactiveSelectionBackground": "#222222",
    "list.inactiveSelectionForeground": "#d7dae0",
    "list.warningForeground": "#CABD2B",
    "menu.background": "#000000",
    "menu.foreground": "#aaaaaa",
    "minimap.selectionHighlight": "#006600",
    "panel.border": "#080800",
    "peekView.border": "#006600",
    "peekViewEditor.background": "#000000",
    "peekViewEditor.matchHighlightBackground": "#29244b",
    "peekViewResult.background": "#000000",
    "peekViewResult.selectionBackground": "#333333",
    "peekViewTitle.background": "#000000",
    "peekViewTitleLabel.foreground": "#aaaaaa",
    "progressBar.background": "#ffd700",
    "scrollbar.shadow": "#00000000",
    "scrollbarSlider.activeBackground": "#747d9180",
    "scrollbarSlider.background": "#4e566660",
    "scrollbarSlider.hoverBackground": "#5a637580",
    "sideBar.background": "#080800",
    "sideBarSectionHeader.background": "#111111",
    "statusBar.background": "#006600",
    "statusBar.debuggingBackground": "#9b2c2c",
    "statusBar.debuggingBorder": "#66017a",
    "statusBar.debuggingForeground": "#fdf7f7",
    "statusBar.foreground": "#f5fff5",
    "statusBar.noFolderBackground": "#553c9a",
    "statusBar.noFolderForeground": "#ede9f6",
    "statusBarItem.hoverBackground": "#00000044",
    "statusBarItem.remoteBackground": "#660066",
    "statusBarItem.remoteForeground": "#fff5ff",
    "tab.activeBackground": "#222222",
    "tab.activeForeground": "#dcdcdc",
    "tab.activeModifiedBorder": "#553c9a",
    "tab.border": "#080800",
    "tab.hoverBackground": "#333333",
    "tab.hoverBorder": "#53ac53",
    "tab.inactiveBackground": "#080800",
    "tab.inactiveModifiedBorder": "#9b2c2c",
    "tab.unfocusedHoverBackground": "#323842",
    "terminal.ansiBlack": "#2d3139",
    "terminal.ansiBlue": "#528bff",
    "terminal.ansiBrightBlack": "#7f848e",
    "terminal.ansiBrightBlue": "#528bff",
    "terminal.ansiBrightCyan": "#0FA5D7",
    "terminal.ansiBrightGreen": "#77B34D",
    "terminal.ansiBrightMagenta": "#7e0097",
    "terminal.ansiBrightRed": "#f44747",
    "terminal.ansiBrightWhite": "#d7dae0",
    "terminal.ansiBrightYellow": "#CABD2B",
    "terminal.ansiCyan": "#0FA5D7",
    "terminal.ansiGreen": "#77B34D",
    "terminal.ansiMagenta": "#B285E0",
    "terminal.ansiRed": "#f44747",
    "terminal.ansiWhite": "#d7dae0",
    "terminal.ansiYellow": "#CABD2B",
    "terminal.foreground": "#c8c8c8",
    "textLink.foreground": "#19AE70",
    "titleBar.activeBackground": "#080800",
    "titleBar.activeForeground": "#aaaaaa",
    "titleBar.inactiveBackground": "#080800",
    "titleBar.inactiveForeground": "#999999",
    "widget.shadow": "#00000000"
  },
  "tokenColors": [
    {
      "name": "Function call and variable declaration",
      "scope": [
        "entity.name.function-call",
        "support.function",
        "entity.other.attribute-name",
        "punctuation.definition.entity",
        "punctuation.separator.key-value",
        "meta.selector"
      ],
      "settings": {
        "foreground": "#E08D52"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.other.assignment", "meta.property-value"],
      "settings": {
        "foreground": "#0fb3b3"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.control.module",
        "constant.other.symbol",
        "keyword.control",
        "keyword.other.special-method",
        "meta.tag.metadata",
        "support.type.property-name",
        "punctuation.separator.key-value.css"
      ],
      "settings": {
        "foreground": "#B285E0"
      }
    },
    {
      "name": "embedded punctuations",
      "scope": ["punctuation.section.embedded"],
      "settings": {
        "foreground": "#ffbb99"
      }
    },
    {
      "name": "Module",
      "scope": ["storage.type", "entity.name.type.module", "variable.other.constant", "meta.tag.metadata.style"],
      "settings": {
        "foreground": "#0FA5D7"
      }
    },
    {
      "name": "Function declaration and definition",
      "scope": [
        "entity.name.function",
        "entity.name.function",

        "support.constant.property-value",
        "support.constant.color.w3c-standard-color-name",
        "meta.tag.metadata.link"
      ],
      "settings": {
        "foreground": "#19AE70"
      }
    },
    {
      "name": "Variable, parameter, seperator, Built-in functons",
      "scope": [
        "punctuation.definition.variable",
        "variable.other.readwrite.module",
        "punctuation.separator.object",
        "support.function.builtin",
        "meta.tag.metadata.title"
      ],
      "settings": {
        "foreground": "#f075b3"
      }
    },
    {
      "name": "Comparison and logical operators",
      "scope": ["keyword.operator.comparison", "meta.tag.structure"],
      "settings": {
        "foreground": "#f76e6e"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "punctuation.definition.string",
        "string.quoted.double",
        "source.elixir.embedded.source",
        "entity.name.import"
      ],
      "settings": {
        "foreground": "#99b34d"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant", "constant.language", "meta.tag.metadata.script", "keyword.other.unit"],
      "settings": {
        "foreground": "#d2b72d"
      }
    },
    {
      "scope": ["source", "keyword.operator", "keyword.operator.other"],
      "settings": {
        "foreground": "#B5B5B5"
      }
    },
    {
      "scope": ["punctuation.section.function"],
      "settings": {
        "foreground": "#a5a5a5"
      }
    },
    {
      "name": "Less important punctuations",
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "#999999"
      }
    },
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#444444",
        "fontStyle": "italic"
      }
    }
  ]
}
