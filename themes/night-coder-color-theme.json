{
  "$schema": "vscode://schemas/color-theme",
  "name": "Night Coder",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "activityBar.activeBorder": "#030917",
    "activityBar.background": "#030917",
    "activityBar.foreground": "#acaeb3",
    "activityBarBadge.background": "#006600",
    "activityBarBadge.foreground": "#f5fff5",
    "badge.background": "#006600",
    "button.background": "#353a45",
    "debugToolBar.background": "#474c56",
    "diffEditor.insertedTextBackground": "#00660033",
    "dropdown.background": "#151a27",
    "dropdown.border": "#030917",
    "editor.background": "#030917",
    "editor.findMatchBackground": "#242935",
    "editor.findMatchBorder": "#53ac53",
    "editor.findMatchHighlightBackground": "#353a4577",
    "editor.foldBackground": "#0066001f",
    "editor.foreground": "#c5c6c9",
    "editor.lineHighlightBackground": "#353a4577",
    "editor.selectionBackground": "#353a4599",
    "editor.selectionHighlightBackground": "#353a4577",
    "editor.selectionHighlightBorder": "#cdced1",
    "editor.wordHighlightBackground": "#353a4577",
    "editor.wordHighlightBorder": "#cdced1",
    "editor.wordHighlightStrongBackground": "#bbbcc022",
    "editor.wordHighlightStrongBorder": "#dedfe1",
    "editorLineNumber.activeForeground": "#acaeb3",
    "editorBracketMatch.background": "#565a63",
    "editorBracketMatch.border": "#565a63",
    "editorCursor.background": "#151a27",
    "editorCursor.foreground": "#edeeef",
    "editorError.background": "#FF757533",
    "editorError.foreground": "#FF7575",
    "editorGroup.border": "#030917",
    "editorGroupHeader.tabsBackground": "#030917",
    "editorHoverWidget.background": "#242935",
    "editorHoverWidget.border": "#030917",
    "editorIndentGuide.activeBackground": "#53ac53",
    "editorIndentGuide.background": "#353a45",
    "editorInfo.background": "#80bfff33",
    "editorInfo.foreground": "#80bfff",
    "editorLineNumber.foreground": "#565a63",
    "editorMarkerNavigation.background": "#242935",
    "editorOverviewRuler.border": "#030917",
    "editorRuler.foreground": "#bbbcc026",
    "editorSuggestWidget.background": "#151a27",
    "editorSuggestWidget.border": "#030917",
    "editorSuggestWidget.foreground": "#cdced1",
    "editorSuggestWidget.highlightForeground": "#edeeef",
    "editorSuggestWidget.selectedBackground": "#565a63",
    "editorWarning.background": "#c3a82233",
    "editorWarning.foreground": "#c3a822",
    "editorWhitespace.foreground": "#686b74",
    "editorWidget.background": "#242935",
    "editorWidget.border": "#030917",
    "editorWidget.resizeBorder": "#006600",
    "focusBorder": "#030917",
    "input.background": "#151a27",
    "list.activeSelectionBackground": "#353a45",
    "list.activeSelectionForeground": "#cdced1",
    "list.focusBackground": "#353a45",
    "list.highlightForeground": "#edeeef",
    "list.hoverBackground": "#474c56",
    "list.inactiveSelectionBackground": "#242935",
    "list.inactiveSelectionForeground": "#dedfe1",
    "list.warningForeground": "#c3a822",
    "menu.background": "#151a27",
    "menu.foreground": "#acaeb3",
    "minimap.selectionHighlight": "#006600",
    "panel.border": "#030917",
    "peekView.border": "#006600",
    "peekViewEditor.background": "#151a27",
    "peekViewEditor.matchHighlightBackground": "#353a45",
    "peekViewResult.background": "#151a27",
    "peekViewResult.selectionBackground": "#353a45",
    "peekViewTitle.background": "#151a27",
    "peekViewTitleLabel.foreground": "#acaeb3",
    "progressBar.background": "#c3a822",
    "scrollbar.shadow": "#151a2700",
    "scrollbarSlider.activeBackground": "#006600ff",
    "scrollbarSlider.background": "#474c5677",
    "scrollbarSlider.hoverBackground": "#565a6388",
    "sideBar.background": "#030917",
    "sideBarSectionHeader.background": "#151a27",
    "statusBar.background": "#006600",
    "statusBar.debuggingBackground": "#9b2c2c",
    "statusBar.debuggingBorder": "#770000",
    "statusBar.debuggingForeground": "#fdf7f7",
    "statusBar.foreground": "#f5fff5",
    "statusBar.noFolderBackground": "#553c9a",
    "statusBar.noFolderForeground": "#ede9f6",
    "statusBarItem.hoverBackground": "#151a2744",
    "statusBarItem.remoteBackground": "#660066",
    "statusBarItem.remoteForeground": "#fff5ff",
    "tab.activeBackground": "#242935",
    "tab.activeForeground": "#dedfe1",
    "tab.activeModifiedBorder": "#553c9a",
    "tab.border": "#030917",
    "tab.hoverBackground": "#353a45",
    "tab.hoverBorder": "#53ac53",
    "tab.inactiveBackground": "#030917",
    "tab.inactiveModifiedBorder": "#9b2c2c",
    "tab.unfocusedHoverBackground": "#272c38",
    "terminal.ansiBlack": "#353a45",
    "terminal.ansiBlue": "#859DC3",
    "terminal.ansiBrightBlack": "#888b92",
    "terminal.ansiBrightBlue": "#80bfff",
    "terminal.ansiBrightCyan": "#0FA5D7",
    "terminal.ansiBrightGreen": "#89B971",
    "terminal.ansiBrightMagenta": "#F28CA6",
    "terminal.ansiBrightRed": "#EF7B6D",
    "terminal.ansiBrightWhite": "#c5c6c9",
    "terminal.ansiBrightYellow": "#f5d780",
    "terminal.ansiCyan": "#0FA5D7",
    "terminal.ansiGreen": "#85C3AB",
    "terminal.ansiMagenta": "#C385BC",
    "terminal.ansiRed": "#FF7575",
    "terminal.ansiWhite": "#acaeb3",
    "terminal.ansiYellow": "#d9d326",
    "terminal.foreground": "#acaeb3",
    "textLink.foreground": "#89B971",
    "titleBar.activeBackground": "#030917",
    "titleBar.activeForeground": "#acaeb3",
    "titleBar.inactiveBackground": "#030917",
    "titleBar.inactiveForeground": "#9a9da2",
    "widget.shadow": "#151a2700"
  },
  "semanticTokenColors": {
    // "variable": "#FFA666",
    "variable": "#C5C6C9",
    "variable.readonly": "#FF7575",
    // "variable.defaultLibrary": "#ffd3b3",
    "variable.defaultLibrary": "#FF99B3",
    "property": "#FFA666",
    // "property": "#70DBC1",
    "function.declaration": "#85C3AB",
    "method.declaration": "#85C3AB",
    "keyword": "#C385BC",
    "namespace": "#859DC3",
    "class": "#859DC3",
    "struct": "#859DC3",
    "type": "#80BFFF",
    "operator": "#F2F28C",
    "string": "#9ED279"
    // "variable.defaultLibrary": "#ACA3DC",
    // "variable.readonly": "#aca3dc",
  },
  "tokenColors": [
    {
      "name": "variable",
      "scope": [
        "variable.graphql",
        "variable.object.property",
        "variable.other.assignment",
        "variable.other.block.crystal",
        "variable.other.block",
        "variable.other.bracket.shell",
        "variable.other.declaration",
        "variable.other.normal.shell",
        "variable.other.object.js.jsx",
        "variable.other.object.property",
        "variable.other.object.tsx",
        "variable.other.object",
        "variable.other.php",
        "variable.other.positional.shell",
        "variable.other.property",
        "variable.other.readwrite.instance.crystal",
        "variable.other.readwrite",
        "variable.other.ruby",
        "variable.other",
        "variable.scss",
        "meta.function-call.arguments" // TODO python
      ],
      "settings": {
        "foreground": "#C5C6C9"
        // "foreground": "#FFA666"
      }
    },
    {
      "name": "special variables",
      "scope": [
        "variable.language.self",
        "variable.language.special.self",
        "variable.language.super",
        "variable.language.this",
        "variable.other.alias.yaml",
        "variable.other.class.php",
        "variable.other.constant",
        "variable.other.global.safer.php",
        "variable.other.readwrite.class.ruby",
        "variable.other.readwrite.instance.ruby",
        "variable.parameter.function.language.special.self",
        "variable.other.readwrite.instance.crystal"
      ],
      "settings": {
        "foreground": "#FF7575"
      }
    },
    {
      "name": "default library",
      "scope": [
        "support.variable.object.node",
        "meta.property-value", // TODO check this later (css)
        "support.constant.color.w3c-standard-color-name",
        "support.type",
        "support.class",
        "support.other.namespace"
        // "punctuation.definition.decorator.python"
      ],
      "settings": {
        "foreground": "#FF99B3"
      }
    },
    {
      "name": "non variable constant",
      "scope": [
        // "support.constant.property-value", // TODO check this later (css)
        "constant",
        // "constant.language",
        "support.constant",
        "keyword.other.unit"
      ],
      "settings": {
        // "foreground": "#ACA3DC"
        "foreground": "#AF8FEF"
      }
    },
    {
      "name": "property, key",
      "scope": [
        "meta.object-literal.key",
        "support.type.property-name.json",
        "variable.other.property.js.jsx",
        "variable.other.property.tsx",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#FFA666"
        // "foreground": "#70DBC1"
      }
    },
    {
      "name": "function, method",
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#85C3AB"
      }
    },
    {
      "name": "keyword",
      "scope": [
        "keyword.control.module",
        "keyword.control",
        "keyword.operator.expression.of",
        "keyword.operator.new",
        "keyword.other.special-method",
        "keyword.type.go",
        "keyword",
        "storage.modifier",
        "storage.type.class",
        "storage.type.function",
        "storage.type.js"
      ],
      "settings": {
        "foreground": "#C385BC"
      }
    },
    {
      "name": "namespace, type, struct, class, module",
      "scope": [
        "entity.name.package",
        "entity.name.type",
        "entity.name.class",
        "entity.name.namespace",
        "entity.name.type.cpp",
        "storage.modifier.package.java"
      ],
      "settings": {
        "foreground": "#859dc3"
      }
    },
    {
      "name": "logical and comparison operators",
      "scope": ["keyword.operator.comparison", "keyword.operator.logical"],
      "settings": {
        "foreground": "#93ecb8"
      }
    },
    {
      "name": "types",
      "scope": [
        // "support.type.swift",
        "storage.type",
        "keyword.type"
      ],
      "settings": {
        "foreground": "#80BFFF"
      }
    },
    {
      "name": "operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#f2f28c"
      }
    },
    {
      "name": "String",
      "scope": ["source.elixir.embedded.source", "storage.modifier.import.java", "string.quoted.double", "string"],
      "settings": {
        "foreground": "#9ED279"
      }
    },
    {
      "name": "punctuation",
      "scope": [
        "meta.brace",
        // "punctuation.definition.arguments",
        // "punctuation.definition.begin.bracket.round",
        // "punctuation.definition.begin.bracket",
        // "punctuation.definition.block",
        // "punctuation.definition.bracket.square",
        // "punctuation.definition.end.bracket.round",
        // "punctuation.definition.end.bracket",
        // "punctuation.definition.list",
        // "punctuation.definition.parameters",
        // "punctuation.definition.decorator",
        // "punctuation.other",
        // "punctuation.parenthesis",
        // "punctuation.section.array",
        // "punctuation.section.class", // python
        // "punctuation.section.function",
        // "punctuation.section.property-list",
        // "punctuation.section.scope",
        // "punctuation.separator.key-value.css",
        // "punctuation.separator.list",
        // "punctuation.separator.object"
        "punctuation"
      ],
      "settings": {
        "foreground": "#acaeb3"
      }
    },
    {
      "name": "embedded punctuation",
      "scope": ["punctuation.definition.template-expression", "punctuation.section.embedded"],
      "settings": {
        "foreground": "#FF7575"
      }
    },
    {
      "scope": ["comment", "punctuation.definition.comment", "string.quoted.docstring.multi.python"],
      "settings": {
        "foreground": "#474c56",
        "fontStyle": "italic"
      }
    },

    // markup languages

    {
      "scope": ["meta.tag.metadata", "markup.underline.link"],
      "settings": {
        "foreground": "#C385BC"
      }
    },
    {
      "scope": ["meta.tag.metadata.link", "string.other.link.title.markdown"],
      "settings": {
        "foreground": "#85c3ab"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#93ecb8",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "#7a7d84"
      }
    },

    // ####################################################################################################################################
    // ####################################################################################################################################
    // ####################################################################################################################################
    // ####################################################################################################################################
    // ####################################################################################################################################

    {
      "name": "function call",
      "scope": [
        "entity.name.function-call",
        "meta.function-call",
        "support.function",
        "entity.other.attribute-name",
        "meta.method-call entity.name.function",
        "meta.function-call entity.name.function",
        // "punctuation.separator.key-value",
        "heading"
      ],
      "settings": {
        "foreground": "#f5d780"
      }
    },
    {
      "scope": ["meta.tag.object"],
      "settings": {
        "foreground": "#ff99b3"
      }
    },
    {
      "name": "Module, class, package",
      "scope": [
        "meta.tag.metadata.style",
        "string.other.link.description.markdown",
        "source.vue entity.name.tag.style.html",
        "source.graphql support.type",
        "storage.class.std"
      ],
      "settings": {
        "foreground": "#859dc3"
      }
    },
    {
      "scope": [
        "keyword.control.pseudo-method",
        "meta.tag.metadata.script",
        "source.vue entity.name.tag.script.html",
        "meta.generic-where-clause.swift" // TODO swift
      ],
      "settings": {
        // "foreground": "#ACA3DC"
        "foreground": "#c3a822"
      }
    },
    {
      "scope": [
        "entity.alias.import",
        "invalid.illegal.tag-handle.yaml",
        "text.html.markdown entity.name.tag.html",
        "text.html.markdown punctuation.definition.tag",
        // "punctuation.definition.template-expression",
        "source.css support.function",
        "meta.type_params",
        "entity.name.tag.template.html",
        "meta.inheritance-clause",
        "meta.tag.other.unrecognized",
        "storage.type.class.po"
      ],
      "settings": {
        "foreground": "#ff7575"
      }
    },
    {
      "scope": ["meta.tag.custom"],
      "settings": {
        "foreground": "#d9d326"
      }
    },
    {
      "scope": ["meta.tag.object", "entity.name.scope-resolution", "markup.bold"],
      "settings": {
        "foreground": "#80bfff"
      }
    },
    {
      "scope": [
        "entity.name.tag.yaml",
        "meta.tag.inline",
        "entity.name.tag.js.jsx",
        "entity.name.tag.tsx",
        "keyword.key.toml"
      ],
      "settings": {
        "foreground": "#70dbc1"
      }
    },
    {
      "scope": [
        "meta.tag.structure",
        "variable.other.readwrite.module",
        "entity.name.tag.css",
        "source.css entity.other.attribute-name",
        "meta.selector.css",
        "source.css.scss entity.name.tag.css",
        "markup.fenced_code.block",
        "markup.inline.raw.string",
        "markup.raw.block",
        "entity.other.inherited-class",
        "fenced_code.block.language",
        "entity.name.tag.localname.cs",
        // "punctuation.definition.annotation",
        "storage.type.annotation",
        // "punctuation.section.embedded",
        "entity.name.function.member.cpp",
        "meta.attribute",
        "meta.tag",
        "support.function.builtin"
      ],
      "settings": {
        "foreground": "#ffa666"
      }
    },
    {
      "scope": ["comment.wildcard", "meta.jsx.children"],
      "settings": {
        "foreground": "#c5c6c9"
      }
    },
    {
      "scope": ["markup.quote.markdown", "punctuation.definition.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["punctuation.definition.bold", "markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    }
  ]
}
