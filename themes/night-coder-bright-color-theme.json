{
  "$schema": "vscode://schemas/color-theme",
  "name": "Night Coder Bright",
  "type": "dark",
  "colors": {
    "activityBar.activeBorder": "#080800",
    "activityBar.background": "#080800",
    "activityBar.foreground": "#aaaaaa",
    "activityBarBadge.background": "#006600",
    "activityBarBadge.foreground": "#f5fff5",
    "badge.background": "#006600",
    "button.background": "#333333",
    "debugToolBar.background": "#444444",
    "diffEditor.insertedTextBackground": "#00660033",
    "dropdown.background": "#111111",
    "dropdown.border": "#080800",
    "editor.background": "#080800",
    "editor.findMatchBackground": "#ffffff22",
    "editor.findMatchBorder": "#53ac53",
    "editor.findMatchHighlightBackground": "#33333377",
    "editor.foreground": "#C5c5c5",
    "editor.lineHighlightBackground": "#33333377",
    "editor.selectionBackground": "#33333399",
    "editor.selectionHighlightBackground": "#33333377",
    "editor.selectionHighlightBorder": "#cccccc",
    "editor.wordHighlightBackground": "#33333377",
    "editor.wordHighlightBorder": "#cccccc",
    "editor.wordHighlightStrongBackground": "#BBBBBB22",
    "editor.wordHighlightStrongBorder": "#dddddd",
    "editorLineNumber.activeForeground": "#aaa",
    "editorBracketMatch.background": "#555",
    "editorBracketMatch.border": "#555",
    "editorCursor.background": "#ffffffdd",
    "editorCursor.foreground": "#eeeeee",
    "editorError.foreground": "#f76e6e",
    "editorGroup.emptyBackground": "#080800",
    "editorGroup.border": "#080800",
    "editorGroupHeader.tabsBackground": "#080800",
    "editorHoverWidget.background": "#222222",
    "editorHoverWidget.border": "#080800",
    "editorIndentGuide.activeBackground": "#53ac53",
    "editorIndentGuide.background": "#333333",
    "editorLineNumber.foreground": "#555555",
    "editorMarkerNavigation.background": "#222222",
    "editorOverviewRuler.border": "#080800",
    "editorRuler.foreground": "#BBBBBB26",
    "editorSuggestWidget.background": "#000000",
    "editorSuggestWidget.border": "#080800",
    "editorSuggestWidget.foreground": "#cccccc",
    "editorSuggestWidget.highlightForeground": "#eeeeee",
    "editorSuggestWidget.selectedBackground": "#555555",
    "editorWarning.foreground": "#CABD2B",
    "editorWhitespace.foreground": "#666666",
    "editorWidget.background": "#000000",
    "editorWidget.border": "#080800",
    "editorWidget.resizeBorder": "#006600",
    "focusBorder": "#080800",
    "input.background": "#111111",
    "list.activeSelectionBackground": "#333333",
    "list.activeSelectionForeground": "#cccccc",
    "list.focusBackground": "#333333",
    "list.highlightForeground": "#eeeeee",
    "list.hoverBackground": "#444444",
    "list.inactiveSelectionBackground": "#222222",
    "list.inactiveSelectionForeground": "#dddddd",
    "list.warningForeground": "#CABD2B",
    "menu.background": "#000000",
    "menu.foreground": "#aaaaaa",
    "minimap.selectionHighlight": "#006600",
    "panel.border": "#080800",
    "peekView.border": "#006600",
    "peekViewEditor.background": "#000000",
    "peekViewEditor.matchHighlightBackground": "#333333",
    "peekViewResult.background": "#000000",
    "peekViewResult.selectionBackground": "#333333",
    "peekViewTitle.background": "#000000",
    "peekViewTitleLabel.foreground": "#aaaaaa",
    "progressBar.background": "#ffd700",
    "scrollbar.shadow": "#00000000",
    "scrollbarSlider.activeBackground": "#006600ff",
    "scrollbarSlider.background": "#44444477",
    "scrollbarSlider.hoverBackground": "#55555588",
    "sideBar.background": "#080800",
    "sideBarSectionHeader.background": "#111111",
    "statusBar.background": "#006600",
    "statusBar.debuggingBackground": "#9b2c2c",
    "statusBar.debuggingBorder": "#770000",
    "statusBar.debuggingForeground": "#fdf7f7",
    "statusBar.foreground": "#f5fff5",
    "statusBar.noFolderBackground": "#553c9a",
    "statusBar.noFolderForeground": "#ede9f6",
    "statusBarItem.hoverBackground": "#00000044",
    "statusBarItem.remoteBackground": "#660066",
    "statusBarItem.remoteForeground": "#fff5ff",
    "tab.activeBackground": "#222222",
    "tab.activeForeground": "#dddddd",
    "tab.activeModifiedBorder": "#553c9a",
    "tab.border": "#080800",
    "tab.hoverBackground": "#333333",
    "tab.hoverBorder": "#53ac53",
    "tab.inactiveBackground": "#080800",
    "tab.inactiveModifiedBorder": "#9b2c2c",
    "tab.unfocusedHoverBackground": "#252525",
    "terminal.ansiBlack": "#333333",
    "terminal.ansiBlue": "#859DC3",
    "terminal.ansiBrightBlack": "#888888",
    "terminal.ansiBrightBlue": "#859DC3",
    "terminal.ansiBrightCyan": "#0FA5D7",
    "terminal.ansiBrightGreen": "#89B971",
    "terminal.ansiBrightMagenta": "#F28CA6",
    "terminal.ansiBrightRed": "#EF7B6D",
    "terminal.ansiBrightWhite": "#c5c5c5",
    "terminal.ansiBrightYellow": "#E5B148",
    "terminal.ansiCyan": "#0FA5D7",
    "terminal.ansiGreen": "#85C3AB",
    "terminal.ansiMagenta": "#C385BC",
    "terminal.ansiRed": "#EF7B6D",
    "terminal.ansiWhite": "#aaaaaa",
    "terminal.ansiYellow": "#E7955A",
    "terminal.foreground": "#aaaaaa",
    "textLink.foreground": "#89B971",
    "titleBar.activeBackground": "#080800",
    "titleBar.activeForeground": "#aaaaaa",
    "titleBar.inactiveBackground": "#080800",
    "titleBar.inactiveForeground": "#999999",
    "widget.shadow": "#00000000"
  },
  "tokenColors": [
    {
      "name": "function call",
      "scope": [
        "entity.name.function-call",
        "meta.function-call",
        "support.function",
        "entity.other.attribute-name",
        "variable.other.alias.yaml",
        "meta.method-call entity.name.function",
        "meta.function-call entity.name.function",
        "markup.italic",
        "punctuation.separator.key-value"
      ],
      "settings": {
        "foreground": "#f6dd93"
      }
    },
    {
      "scope": [
        "variable.other.block",
        "variable.other.declaration",
        "entity.name.type.template",
        "support.type.primitive",
        "variable.other.php"
      ],
      "settings": {
        "foreground": "#ff99b3"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "constant.other.symbol",
        "keyword.control.module",
        "keyword.control",
        "keyword.operator.expression.of",
        "keyword.operator.new",
        "keyword.other.special-method",
        "keyword",
        "markup.underline.link",
        "meta.tag.metadata",
        "storage.modifier",
        "storage.type.class",
        "storage.type.function",
        "storage.type.js"
      ],
      "settings": {
        "foreground": "#c385bc"
      }
    },
    {
      "name": "Module, class, package",
      "scope": [
        "constant.language.symbol",
        "entity.name.package",
        "entity.name.type",
        "entity.name.class",
        "meta.property-value",
        "meta.tag.metadata.style",
        "punctuation.section.class",
        "storage.type",
        "string.other.link.description.markdown",
        "support.class",
        "support.constant.property-value",
        "variable.other.constant",
        "support.other.namespace",
        "entity.name.namespace",
        "source.vue entity.name.tag.style.html",
        "source.graphql support.type"
      ],
      "settings": {
        "foreground": "#859dc3"
      }
    },
    {
      "name": "Function declaration and definition",
      "scope": [
        "entity.name.function",
        "meta.tag.metadata.link",
        "string.other.link.title.markdown",
        "support.constant.color.w3c-standard-color-name"
      ],
      "settings": {
        "foreground": "#85c3ab"
      }
    },
    {
      "scope": [
        "keyword.control.pseudo-method",
        "meta.tag.metadata.script",
        "source.vue entity.name.tag.script.html",
        "support.type.python",
        "variable.language.super",
        "meta.generic-where-clause.swift",
        "constant",
        "constant.language",
        "support.constant",
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#d0b425"
      }
    },
    {
      "name": "Comparison and logical operators",
      "scope": [
        "entity.alias.import",
        "entity.name.type.cpp",
        "invalid.illegal.tag-handle.yaml",
        "text.html.markdown entity.name.tag.html",
        "text.html.markdown punctuation.definition.tag",
        "punctuation.definition.template-expression",
        "source.css support.function",
        "support.variable.object.node",
        "variable.language.self",
        "variable.language.special.self",
        "variable.language.this",
        "variable.other.readwrite.class.ruby",
        "variable.other.readwrite.instance.ruby",
        "variable.parameter.function.language.special.self",
        "meta.type_params",
        "variable.other.readwrite.instance.crystal",
        "variable.other.positional.shell",
        "variable.other.normal.shell",
        "entity.name.tag.template.html",
        "meta.inheritance-clause",
        "variable.scss"
      ],
      "settings": {
        "foreground": "#ff7575"
      }
    },
    {
      "scope": ["storage.class.std", "entity.name.scope-resolution", "support.type.swift", "heading"],
      "settings": {
        "foreground": "#99bfe6"
      }
    },
    {
      "name": "String",
      "scope": [
        "entity.name.import",
        // "punctuation.definition.string",
        "source.elixir.embedded.source",
        "storage.modifier.import",
        "string.quoted.double",
        "string"
      ],
      "settings": {
        "foreground": "#89B971"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#f2f28c"
      }
    },
    {
      "scope": ["keyword.operator.comparison", "keyword.operator.logical"],
      "settings": {
        "foreground": "#baf7e3"
      }
    },
    {
      "scope": [
        "meta.object-literal.key",
        "variable.other.global.safer.php",
        "variable.other.class.php",
        "variable.other.assignment"
      ],
      "settings": {
        "foreground": "#93ecb8"
      }
    },
    {
      "scope": [
        "meta.tag.structure",
        "variable.other.readwrite.module",
        "variable.other.readwrite",
        "variable.other.object.property",
        "variable.other.object",
        "variable.other.property",
        "variable.other.bracket.shell",
        "variable.other.object.js.jsx",
        "variable.other.object.tsx",
        "support.type.property-name.json",
        "entity.name.tag.yaml",
        "entity.name.tag.css",
        "source.css entity.other.attribute-name",
        "meta.selector.css",
        "source.css.scss entity.name.tag.css",
        "markup.fenced_code.block",
        "markup.inline.raw.string",
        "markup.raw.block",
        "entity.other.inherited-class",
        "fenced_code.block.language",
        "entity.name.tag.localname.cs",
        "variable.graphql",
        "punctuation.definition.annotation",
        "storage.type.annotation",
        "punctuation.section.embedded",
        "entity.name.function.member.cpp",
        "variable.object.property",
        "meta.attribute",
        "meta.tag",
        "support.function.builtin"
      ],
      "settings": {
        "foreground": "#ffa375"
        // "foreground": "#ff9966"
      }
    },
    {
      "scope": [
        "comment.wildcard",
        "support.type.property-name",
        "variable.other.ruby",
        "variable.other.block.crystal",
        "variable.other.property.js.jsx",
        "variable.other.property.tsx",
        "variable.other"
      ],
      "settings": {
        "foreground": "#C5c5c5"
      }
    },
    {
      "scope": [
        "markup.bold",
        "meta.brace",
        "punctuation.definition.arguments",
        "punctuation.definition.begin.bracket.round",
        "punctuation.definition.begin.bracket",
        "punctuation.definition.block",
        "punctuation.definition.bracket.square",
        "punctuation.definition.end.bracket.round",
        "punctuation.definition.end.bracket",
        "punctuation.definition.list",
        "punctuation.definition.parameters",
        "punctuation.other",
        "punctuation.parenthesis",
        "punctuation.section.array",
        "punctuation.section.function",
        "punctuation.section.property-list",
        "punctuation.section.scope",
        "punctuation.separator.key-value.css",
        "punctuation.separator.list",
        "punctuation.separator.object"
        // "punctuation.separator"
      ],
      "settings": {
        "foreground": "#aaaaaa"
      }
    },
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#444444",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["punctuation"],
      "settings": {
        // "fontStyle": ""
      }
    },
    {
      "scope": [
        "markup.italic",
        "markup.quote.markdown",
        "punctuation.definition.italic",
        "variable.language.self",
        "variable.language.special.self",
        "variable.language.this",
        "variable.parameter.function.language.special.self"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["punctuation.definition.bold", "markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    }
  ]
}
